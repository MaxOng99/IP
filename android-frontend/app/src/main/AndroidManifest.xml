<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.touchauthenticator">

    <uses-permission android:name="android.perimssion.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="testphase-feature"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.auth.AuthActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.home.HomeActivity">
        </activity>
        <activity android:name=".ui.enrolment.EnrolmentActivity">
        </activity>
        <activity android:name=".ui.enrolment.ReactionEnrolmentActivity">
        </activity>
        <activity android:name=".ui.enrolment.KeystrokeEnrolmentActivity">
        </activity>
        <activity android:name=".ui.test.KeystrokeTestActivity">
        </activity>
        <activity android:name=".ui.test.ReactionTestActivity">
        </activity>
        <activity android:name=".ui.result.ResultActivity">
        </activity>
    </application>

</manifest>